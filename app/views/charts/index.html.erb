<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header"><%=I18n.t(:charts, scope: :generic)%></h1>
  </div>
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%=I18n.t(:age_distribution, scope: :charts)%>
      </div>
      <div class="panel-body">
        <%= line_chart "15" => 10, "20" => 30, "25" => 10, "30" => 5, 35 => 10 %>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-lg-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%=I18n.t(:gender_distribution, scope: :charts)%>
      </div>
      <div class="panel-body">
        <%= pie_chart "Female" => 50, "Male" => 45 %>
      </div>
    </div>
  </div>
</div>
